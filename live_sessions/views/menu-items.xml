<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <menuitem
        id="live_session_root_menu"
        name="Live Session"
        action="test_action"
        />

    <menuitem
        id="live_session_add_menu"
        name="Add Session"
        parent="live_session_root_menu"
        action="live_session_action_add"/>
    
    <record id="live_session_coach_tree" model='ir.ui.view'>
        <field name="name">Coach Requests Tree View</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"></field>
                <field name="email"></field>
                <field name="qualification"></field>
                <field name="teaching_experience"></field>
            </tree>
        </field>
    </record>


    <record id="live_session_action_request" model="ir.actions.act_window">
        <field name="name">Coach Requests</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="live_sessions.live_session_coach_tree"></field>
    </record>
    
    <menuitem
        id="live_session_request_menu"
        name="Coach Requests"
        parent="live_session_root_menu"
        action="live_session_action_request"/>

    

    
</odoo>
